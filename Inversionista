package sample;

public class Inversionista {

    public String firma;
    public double ganancias;
    public  double inversionSede;
    public Sede sedes;
    public Multinacional multinacional;
    public  double inversionMN;

    public Inversionista(String firma, double ganancias, double inversionMN,double inversionSede) {
        this.firma =firma ;
        this.ganancias = ganancias;
        this.inversionMN = inversionMN;
        this.inversionSede = inversionSede;
        this.multinacional = null;
        this.sedes= null;

    }

}
